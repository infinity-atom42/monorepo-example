{
	"name": "web-example",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"scripts": {
		"auth:generate": "dotenvx run --convention=nextjs -- pnpx @better-auth/cli@latest generate --output src/db/auth-schema.ts --config src/utils/auth.ts --yes",
		"build": "next build",
		"clean": "rm -rf .next .turbo node_modules",
		"db:generate": "dotenvx run --convention=nextjs -- drizzle-kit generate --config=src/scripts/drizzle.config.ts",
		"db:migrate": "dotenvx run --convention=nextjs -- drizzle-kit migrate --config=src/scripts/drizzle.config.ts",
		"db:push": "dotenvx run --convention=nextjs -- drizzle-kit push --config=src/scripts/drizzle.config.ts",
		"db:seed": "dotenvx run --convention=nextjs -- bun run src/scripts/seed.ts",
		"dev": "next dev --turbopack -p 3001",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"start": "next start -p 3001",
		"test": "echo \"Error: no test specified\" && exit 1",
		"type:check": "tsc --noEmit"
	},
	"dependencies": {
		"@daveyplate/better-auth-ui": "^3.2.5",
		"@elysiajs/eden": "^1.4.1",
		"@packages/ui": "workspace:*",
		"@t3-oss/env-nextjs": "^0.13.8",
		"@tanstack/react-query": "^5.90.2",
		"@types/bun": "catalog:",
		"better-auth": "^1.3.27",
		"drizzle-orm": "^0.44.6",
		"elysia": "catalog:",
		"lucide-react": "^0.544.0",
		"next": "catalog:",
		"next-themes": "^0.4.6",
		"pg": "^8.16.3",
		"react": "catalog:",
		"react-dom": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "^1.51.0",
		"@types/node": "catalog:",
		"@types/pg": "^8.15.5",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"drizzle-kit": "^0.31.5",
		"drizzle-seed": "^0.3.1",
		"eslint-plugin-drizzle": "^0.2.3"
	}
}

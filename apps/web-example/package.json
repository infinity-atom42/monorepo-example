{
	"name": "web-example",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"auth:generate": "dotenvx run --convention=nextjs -- pnpx @better-auth/cli@latest generate --output src/db/auth-schema.ts --config src/utils/auth.ts --yes",
		"auth:migrate": "dotenvx run --convention=nextjs -- drizzle-kit migrate --config=src/scripts/auth.config.ts",
		"auth:push": "dotenvx run --convention=nextjs -- drizzle-kit push --config=src/scripts/auth.config.ts",
		"build": "next build",
		"clean": "rm -rf .next .turbo node_modules",
		"db:generate": "dotenvx run --convention=nextjs -- drizzle-kit generate --config=src/scripts/drizzle.config.ts",
		"db:migrate": "dotenvx run --convention=nextjs -- drizzle-kit migrate --config=src/scripts/drizzle.config.ts",
		"db:push": "dotenvx run --convention=nextjs -- drizzle-kit push --config=src/scripts/drizzle.config.ts",
		"db:seed": "dotenvx run --convention=nextjs -- bun run src/scripts/seed.ts",
		"dev": "next dev --turbopack -p 3001",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"start": "next start -p 3001",
		"test": "echo \"Error: no test specified\" && exit 1",
		"type:check": "tsc --noEmit"
	},
	"dependencies": {
		"@daveyplate/better-auth-ui": "catalog:",
		"@elysiajs/eden": "catalog:",
		"@packages/ui": "workspace:*",
		"@t3-oss/env-nextjs": "catalog:",
		"@tanstack/react-query": "catalog:",
		"better-auth": "catalog:",
		"drizzle-orm": "catalog:",
		"elysia": "catalog:",
		"lucide-react": "catalog:",
		"next": "catalog:",
		"pg": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "catalog:",
		"@tailwindcss/postcss": "catalog:",
		"@types/bun": "catalog:",
		"@types/pg": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"drizzle-kit": "catalog:",
		"drizzle-seed": "catalog:",
		"eslint-plugin-drizzle": "catalog:",
		"tailwindcss": "catalog:"
	}
}

{
	"name": "web-example",
	"version": "0.0.0",
	"private": true,
	"type": "module",
	"scripts": {
		"auth:generate": "dotenvx run --convention=nextjs -- pnpx @better-auth/cli@latest generate --output src/auth/drizzle.schema.ts --config src/auth/index.ts --yes",
		"auth:migrate": "dotenvx run --convention=nextjs -- drizzle-kit migrate --config=src/auth/drizzle.config.ts",
		"auth:push": "dotenvx run --convention=nextjs -- drizzle-kit push --config=src/auth/drizzle.config.ts",
		"build": "next build",
		"clean": "rm -rf .next .turbo node_modules test",
		"db:generate": "dotenvx run --convention=nextjs -- drizzle-kit generate --config=src/scripts/drizzle.config.ts",
		"db:migrate": "dotenvx run --convention=nextjs -- drizzle-kit migrate --config=src/scripts/drizzle.config.ts",
		"db:push": "dotenvx run --convention=nextjs -- drizzle-kit push --config=src/scripts/drizzle.config.ts",
		"db:seed": "dotenvx run --convention=nextjs -- bun run src/scripts/seed.ts",
		"dev": "next dev --turbopack -p 3001",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"postinstall": "playwright install --with-deps chromium",
		"start": "next start -p 3001",
		"test": "dotenvx run --convention=nextjs -- playwright test --project=chromium",
		"test:all": "dotenvx run --convention=nextjs -- playwright test",
		"test:ui": "dotenvx run --convention=nextjs -- playwright test --ui --project=chromium",
		"test:ui:all": "dotenvx run --convention=nextjs -- playwright test --ui",
		"test:headed": "dotenvx run --convention=nextjs -- playwright test --project=chromium --headed",
		"test:report": "playwright show-report",
		"type:check": "tsc --noEmit"
	},
	"dependencies": {
		"@daveyplate/better-auth-ui": "catalog:",
		"@elysiajs/eden": "catalog:",
		"@packages/ui": "workspace:*",
		"@t3-oss/env-nextjs": "catalog:",
		"@tanstack/react-query": "catalog:",
		"better-auth": "catalog:",
		"drizzle-orm": "catalog:",
		"elysia": "catalog:",
		"lucide-react": "catalog:",
		"next": "catalog:",
		"pg": "catalog:",
		"react": "catalog:",
		"react-dom": "catalog:",
		"zod": "catalog:"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "catalog:",
		"@playwright/test": "^1.48.0",
		"@tailwindcss/postcss": "catalog:",
		"@types/bun": "catalog:",
		"@types/pg": "catalog:",
		"@types/react": "catalog:",
		"@types/react-dom": "catalog:",
		"drizzle-kit": "catalog:",
		"drizzle-seed": "catalog:",
		"eslint-plugin-drizzle": "catalog:",
		"tailwindcss": "catalog:"
	}
}

{
	"name": "payment-example",
	"version": "0.1.0",
	"private": true,
	"type": "module",
	"module": "src/index.ts",
	"scripts": {
		"build": "bun build --compile --minify --target bun --outfile ./dist/server src/index.ts",
		"build:linux": "bun build --compile --minify --target bun-linux-x64 --outfile ./dist/server src/index.ts",
		"dev": "bun run --watch src/index.ts",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"start": "./dist/server",
		"test": "echo \"Error: no test specified\" && exit 1",
		"type:check": "tsc --noEmit",
		"clean": "rm -rf dist .turbo node_modules"
	},
	"dependencies": {
		"@workspace/shared": "workspace:*",
		"amqp-connection-manager": "^5.0.0",
		"amqplib": "^0.10.9"
	},
	"devDependencies": {
		"@types/amqplib": "^0.10.7",
		"@types/bun": "latest"
	}
}
